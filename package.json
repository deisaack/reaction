{
  "name": "begin",
  "version": "1.0.0",
  "description": "some startpoint for react js",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
    "build": "webpack -d && cp src/index.html myapp/library/templates/index.html && webpack-dev-server --content-base src/ --inline --hot",
    "build:prod": "webpack -p && rm myapp/library/templates/index.html && cp src/index.html myapp/library/templates/index.html"
  },
  "keywords": [
    "reactjs"
  ],
  "author": "Kipkemei Isaac",
  "license": "MIT",
  "dependencies": {
    "jquery": "^2.2.2",
    "lodash": "^4.17.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "superagent": "^3.6.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.8.2"
  }
}
